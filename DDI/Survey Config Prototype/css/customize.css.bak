.banner {
	font-weight:bold;
	font-size: 12px;
	color: #fff;
	letter-spacing:normal;
	background:#8ba3d4;
	/* IE10 Consumer Preview */ 
	background-image: -ms-radial-gradient(center, circle farthest-corner, #FFFFFF 0%, #8ba3d4 100%);
	/* Mozilla Firefox */ 
	background-image: -moz-radial-gradient(center, circle farthest-corner, #FFFFFF 0%, #8ba3d4 100%);
	/* Opera */ 
	background-image: -o-radial-gradient(center, circle farthest-corner, #FFFFFF 0%, #8ba3d4 100%);
	/* Webkit (Safari/Chrome 10) */ 
	background-image: -webkit-gradient(radial, center center, 0, center center, 447, color-stop(0, #FFFFFF), color-stop(1, #8ba3d4));
	/* Webkit (Chrome 11+) */ 
	background-image: -webkit-radial-gradient(center, circle farthest-corner, #FFFFFF 0%, #8ba3d4 100%);
	/* W3C Markup, IE10 Release Preview */ 
	background-image: radial-gradient(circle farthest-corner at center, #FFFFFF 0%, #8ba3d4 100%);
	text-transform: uppercase;	
}

.masthead {
	background-image: url(../img/banner-long.png);
	background-repeat: no-repeat;
	height: 80px;
	}
	
.masthead img {
	margin-left:20px;
	}

.banner-control {
	margin:auto;
	display:block;
	}

.title {
	background: url(../img/bullet.png) repeat-x scroll center bottom transparent;
	color: #fff;
	font-size: 24px;
	font-weight: normal;
	font-variant: small-caps;
	font-family: Times New Roman, Times, serif;
	padding-top:20px;
    left: 500px;
    position: absolute;
	}
	
	
/* http://ie.microsoft.com/testdrive/graphics/cssgradientbackgroundmaker/default.html*/ 


.accordion-toggle {
background-color:#eaeaea;
border-bottom:solid 2px #999999;
	/* IE10 Consumer Preview */ 
background-image: -ms-linear-gradient(top, #FFFFFF 0%, #BFBFBF 200%);

/* Mozilla Firefox */ 
background-image: -moz-linear-gradient(top, #FFFFFF 0%, #BFBFBF 200%);

/* Opera */ 
background-image: -o-linear-gradient(top, #FFFFFF 0%, #BFBFBF 200%);

/* Webkit (Safari/Chrome 10) */ 
background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #FFFFFF), color-stop(2, #BFBFBF));

/* Webkit (Chrome 11+) */ 
background-image: -webkit-linear-gradient(top, #FFFFFF 0%, #BFBFBF 200%);

/* W3C Markup, IE10 Release Preview */ 
background-image: linear-gradient(to bottom, #FFFFFF 0%, #BFBFBF 200%);}

a.accordion-toggle:hover {
	text-decoration:underline;
border-bottom:solid 2px #999999;
	/* IE10 Consumer Preview */ 
background-image: -ms-linear-gradient(top, #FFFFFF 0%, #BFBFBF 200%);

/* Mozilla Firefox */ 
background-image: -moz-linear-gradient(top, #FFFFFF 0%, #BFBFBF 200%);

/* Opera */ 
background-image: -o-linear-gradient(top, #FFFFFF 0%, #BFBFBF 200%);

/* Webkit (Safari/Chrome 10) */ 
background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #FFFFFF), color-stop(2, #BFBFBF));

/* Webkit (Chrome 11+) */ 
background-image: -webkit-linear-gradient(top, #FFFFFF 0%, #BFBFBF 200%);

/* W3C Markup, IE10 Release Preview */ 
background-image: linear-gradient(to bottom, #FFFFFF 0%, #BFBFBF 200%);
}

.accordion-toggle:active {/* IE10 Consumer Preview */ 
font-weight:bold;
color:666666;
border-bottom:solid 2px #3fa8f4;
/* IE10 Consumer Preview */ 
background-image: -ms-linear-gradient(top, #FFFFFF 0%, #3fa8f4 200%);

/* Mozilla Firefox */ 
background-image: -moz-linear-gradient(top, #FFFFFF 0%, #3fa8f4 200%);

/* Opera */ 
background-image: -o-linear-gradient(top, #FFFFFF 0%, #3fa8f4 200%);

/* Webkit (Safari/Chrome 10) */ 
background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #FFFFFF), color-stop(2, #3fa8f4));

/* Webkit (Chrome 11+) */ 
background-image: -webkit-linear-gradient(top, #FFFFFF 0%, #3fa8f4 200%);

/* W3C Markup, IE10 Release Preview */ 
background-image: linear-gradient(to bottom, #FFFFFF 0%, #3fa8f4 200%);
}

.selected {/* IE10 Consumer Preview */ 
border-bottom:solid 2px #3fa8f4;
/* IE10 Consumer Preview */ 
background-image: -ms-linear-gradient(top, #FFFFFF 0%, #3fa8f4 200%);

/* Mozilla Firefox */ 
background-image: -moz-linear-gradient(top, #FFFFFF 0%, #3fa8f4 200%);

/* Opera */ 
background-image: -o-linear-gradient(top, #FFFFFF 0%, #3fa8f4 200%);

/* Webkit (Safari/Chrome 10) */ 
background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #FFFFFF), color-stop(2, #3fa8f4));

/* Webkit (Chrome 11+) */ 
background-image: -webkit-linear-gradient(top, #FFFFFF 0%, #3fa8f4 200%);

/* W3C Markup, IE10 Release Preview */ 
background-image: linear-gradient(to bottom, #FFFFFF 0%, #3fa8f4 200%);
}

.selected a {
font-weight:bold;
color:666666;
}
