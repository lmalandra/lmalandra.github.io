/*buttons*/

.btn {
	color: #333333;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	border: 1px solid #3fa9f4;
	-moz-box-shadow:
		0px 1px 3px rgba(000,000,000,0.5),
		inset 1px 0px 31px rgba(255,255,255,0.9);
	-webkit-box-shadow:
		0px 1px 3px rgba(000,000,000,0.5),
		inset 1px 0px 31px rgba(255,255,255,0.9);
	box-shadow:
		0px 1px 3px rgba(000,000,000,0.5),
		inset 1px 0px 31px rgba(255,255,255,0.9);
text-shadow:none;
		}


.btn:hover {
	background: -moz-linear-gradient(
		top,
		#ffffff 0%,
		#ffffff 15%,
		#3fa9f4);
	background: -webkit-gradient(
		linear, left top, left bottom, 
		from(#ffffff),
		color-stop(0.15, #ffffff),
		to(#3fa9f4));
	
}
.btn-primary{

	background: -moz-linear-gradient(
		top,
		#ffffff 0%,
		#ffffff 15%,
		#3fa9f4);
	background: -webkit-gradient(
		linear, left top, left bottom, 
		from(#ffffff),
		color-stop(0.15, #ffffff),
		to(#3fa9f4));
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	
}
.btn-primary:hover {
	border: 1px solid #3fa9f4;
	color: #000;
	background: -webkit-gradient(
		linear, left top, left bottom, 
		from(#ffffff),
		color-stop(0.35, #ffffff),
		to(#3fa9f4));
}

.btn-primary:focus {
	border: 1px solid #3fa9f4;
	color: #000;
	background: -webkit-gradient(
		linear, left top, left bottom, 
		from(#ffffff),
		color-stop(0.00, #ffffff),
		to(#3fa9f4));
}

.btn-primary.disabled, .btn-primary[disabled] {
    background-color: #E6E6E6;
    color: #333333;
}

.btn-navbar:hover {
	background: -webkit-gradient(
		linear, left top, left bottom, 
		from(#333),
		color-stop(0, #333),
		to(#333)); 
		}

